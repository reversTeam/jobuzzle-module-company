class CompanyDataTemplateViewView extends InstituteDataTemplateViewView {

	_class = 'company';
	_elementType = 'Entreprise';

	display : function () {
		var company = this.state.entity;

		if (company) {
			return <div>View Company : {company.getName()}</div>;
		} else
			return <div>View Company</div>;
	}

}